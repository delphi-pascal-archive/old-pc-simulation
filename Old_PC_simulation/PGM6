470 PRINT "æ"
480 Y1 = 5
490 Y2 = 44
500 FOR X = 5 TO 74
510 SET X,5
520 SET X,44
530 NEXT X
540 X1 = 5
550 X2 = 74
560 FOR Y = 6 TO 44
570 SET X1,Y
580 SET X2,Y
590 NEXT Y
600 RETURN
900 REM
910 FOR N = 1 TO 7
920 ? TAB(T) MID$(A$,P,5)
930 P = P+5
940 NEXT N
