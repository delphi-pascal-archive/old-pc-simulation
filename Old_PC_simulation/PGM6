010 ROPEN "CHIFFRES"
020 INPUT/T A$
030 CLOSE
050 GOSUB 310
100 T = 5
105 D = 0
110 P = D*35+1
120 ? "æ‡‡‡"
130 GOSUB 910
180 T = T+6
190 D = D+1
200 IF D < 5 GOTO 110
210 T = 5
220 P = D*35+1
240 ? "æ‡‡‡‡‡‡‡‡‡‡‡‡"
250 GOSUB 910
260 T = T+6
270 D = D+1
280 IF D < 10 GOTO 220
300 END
310 PRINT "‡‡††½";
320 FOR A = 4 TO 37
330 PRINT "ž";
340 NEXT A
350 PRINT "Ì";
360 FOR A = 4 TO 22
370 PRINT "‡„·";
380 NEXT A
390 PRINT "‡„Ëæ‡‡†††";
400 FOR A = 4 TO 22
410 PRINT "‡„®";
